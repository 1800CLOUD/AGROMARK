<odoo>
    <record id="account_balance_wizard_view_agromark_inherit" model="ir.ui.view">
        <field name="name">account.balance.wizard.view.agromark.inherit</field>
        <field name="model">account.balance.wizard</field>
        <field name="inherit_id" ref="account_report.view_account_balance_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='accounts_ids']" position="after">
               <field name="account_analityc_ids" widget="many2many_tags" 
                attrs="{'invisible': [('balance_analityc','=',False)]}"/>
            </xpath>
            <xpath expr="//field[@name='partner_by']" position="after">
               <field name="balance_analityc" />
            </xpath>
        </field>
    </record>
</odoo>